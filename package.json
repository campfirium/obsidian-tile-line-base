{
  "name": "obsidian-tile-line-base",
  "version": "0.1.0",
  "description": "TileLineBase - A federated paragraph database for Obsidian",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "npx tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings=0",
    "deploy": "npm run build && node scripts/deploy.mjs",
    "deploy:wsl": "npm run build && node scripts/deploy-wsl.mjs",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "database",
    "table"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "eslint": "^8.56.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "ag-grid-community": "^34.2.0"
  }
}
