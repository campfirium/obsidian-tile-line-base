{
  "name": "obsidian-tile-line-base",
  "version": "0.8.3",
  "description": "TileLineBase - A federated paragraph database for Obsidian.",
  "main": "dist/main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "npm run lint && npx tsc -noEmit -skipLibCheck && node esbuild.config.mjs production && npm run check:release",
    "lint": "node scripts/run-lint.mjs",
    "check:i18n-hardcoded": "node scripts/check-i18n-hardcoded.mjs",
    "test:magic": "ts-node --transpile-only --compiler-options '{\"module\":\"commonjs\"}' .lab/scripts/test-magic-migration.ts",
    "deploy": "npm run build && node scripts/deploy.mjs",
    "deploy:wsl": "npm run build && node scripts/deploy-wsl.mjs",
    "mirror:dropbox": "node .lab/scripts/mirror-dropbox-plugin.mjs",
    "version": "node version-bump.mjs && git add package.json manifest.json versions.json",
    "check:release": "node scripts/check-release.mjs"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "database",
    "table"
  ],
  "author": "Campfirium",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "9.39.0",
    "@types/node": "16.18.126",
    "@types/sortablejs": "1.15.9",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "eslint": "9.39.0",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-obsidianmd": "0.1.6",
    "jsonc-eslint-parser": "2.4.1",
    "obsidian": "1.10.0",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.2",
    "ts-node": "10.9.2"
  },
  "dependencies": {
    "ag-grid-community": "34.2.0",
    "sortablejs": "1.15.6"
  }
}
